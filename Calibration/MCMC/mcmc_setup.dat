

#NOTE: currently only supports hamiltonian_monte_carlo, differential_evolution and metropolis_hastings
#algorithm         : metropolis_hastings
#algorithm         : differential_evolution
algorithm         : hamiltonian_monte_carlo

generations       : 1000      #NOTE: excluding burnin

burnin            : 100

discard_timesteps : 365       #NOTE: Skip these many of the first timesteps when doing objective evaluations

# Differential Evolution specific:
chains            : 8
#chains            : 1

de_b              : 0.1       #NOTE: each step each parameter can be perturbed [-de_b, de_b] (uniformly distributed) in the parameter space. Specific to differential evolution.

de_jumps          : true      #NOTE: whether or not DE should perform a (longer) jump every 10 steps

de_jump_gamma     : 1.0       #NOTE: The gamma for the jump (if jumps are enabled)


#HMC Specific
hmc_step_size     : 0.01

hmc_leap_steps    : 2

